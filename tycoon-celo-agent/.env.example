# Tycoon backend base URL (no /api suffix)
TYCOON_API_URL=http://localhost:3000

# Which AI slot this agent backs (2â€“8). e.g. 2 = AI_2
AGENT_SLOT=2

# URL where Tycoon backend can reach this agent (must be reachable from the server)
AGENT_CALLBACK_URL=http://localhost:4077

# ERC-8004 agentId (for Celo / 8004scan). Use a real ID after registering on-chain.
AGENT_ID=tycoon-celo-agent-1

# Celo chain: 42220 = mainnet, 44787 = Alfajores testnet
CELO_CHAIN_ID=42220

# Display name
AGENT_NAME=Tycoon Celo Agent

# Port for this agent server (default 4077)
PORT=4077
